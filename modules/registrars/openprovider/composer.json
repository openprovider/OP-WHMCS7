{
    "autoload": {
        "psr-4": {
            "GuzzleHttp6\\": "vendor/guzzlehttp/guzzle/src/",
            "GuzzleHttp6\\Psr7\\": "vendor/guzzlehttp/psr7/src/",
            "GuzzleHttp6\\Promise\\": "vendor/guzzlehttp/promises/src/",
            "OpenProvider\\WhmcsRegistrar\\": "./",
            "OpenProvider\\": "./OpenProvider"
        },
        "files": [
            "helpers.php",
            "vendor/guzzlehttp/guzzle/src/functions_include.php",
            "vendor/guzzlehttp/psr7/src/functions_include.php",
            "vendor/guzzlehttp/promises/src/functions_include.php"
        ]
    },
    "require": {
        "wedevelopcoffee/wpower": "2.2.*",
        "viison/address-splitter": "^0.3.2",
        "openprovider/rest-client-php": "dev-v1beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "mockery/mockery": "^1.0",
        "php-mock/php-mock-mockery": "^1.2",
        "illuminate/database": "6.20.*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.openprovider.nl"
        }
    ],
    "scripts": {
        "post-update-cmd": "OpenProvider\\WhmcsRegistrar\\scripts\\ComposerUpdateGuzzleNamespace::execute"
    }
}
